            </intent-filter>
        </receiver>
        <receiver android:name=".services.PttButtonReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PTT_KEY_DOWN" />
                <action android:name="android.intent.action.PTT_1" />
                <action android:name="android.intent.action.PTT_2" />
                <action android:name="android.intent.action.PTT_3" />
                <action android:name="com.dfl.s02.pttDown" />
                <action android:name="com.dfl.s02.pttup" />
